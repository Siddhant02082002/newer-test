{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/route/apiFetch.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/route/check-api.js"
      }
    ]
  }
  